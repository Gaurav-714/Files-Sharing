{
  "version": 2,
  "builds": [
      {
          "src": "core/wsgi.py",
          "use": "@vercel/python",
          "config": {
              "maxLambdaSize": "15mb",
              "runtime": "python3.9"
          }
      },
      {
          "src": "build.sh",
          "use": "@vercel/public",
          "config": {
              "distDir": "public"
          }
      }
  ],
  "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/static/$1"
      },
      {
          "src": "/(.*)",
          "dest": "core/wsgi.py"
      }
  ]

}